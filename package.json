{
  "name": "whiskey-wiz",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max-old-space-size=4096 --max-http-header-size=16384 ./server.js",
    "build": "nx build --prod",
    "watch": "nx build --watch --configuration development",
    "test": "nx test",
    "lint": "nx lint",
    "build:elements": "nx build && mkdir -p dist/whiskey-wiz/elements && cp src/app/elements/game-bundle.js dist/whiskey-wiz/elements/game-elements.js",
    "serve:elements": "http-server dist/whiskey-wiz -p 8080",
    "build:all": "npm run build:elements",
    "deploy": "npm run build:elements && firebase deploy --only hosting",
    "clean": "rimraf dist/whiskey-wiz/elements"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/elements": "^17.3.12",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@nrwl/cli": "^15.9.3",
    "@shopify/shopify-api": "^11.6.0",
    "@webcomponents/custom-elements": "^1.6.0",
    "chart.js": "^4.4.7",
    "custom-elements": "^1.0.2",
    "firebase": "^10.8.0",
    "node-sass": "^7.0.1",
    "rxjs": "^7.8.1",
    "sass": "1.80.6",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular-devkit/core": "^17.2.0",
    "@angular-devkit/schematics": "^17.2.0",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.3.11",
    "@angular/compiler-cli": "^17.2.0",
    "@nx/angular": "17.2.0",
    "@nx/workspace": "17.2.0",
    "@schematics/angular": "^17.2.0",
    "@types/chart.js": "^2.9.41",
    "@types/estree": "^1.0.6",
    "@types/http-proxy": "^1.17.15",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/json-schema": "^7.0.15",
    "@types/mute-stream": "^0.0.4",
    "@types/node": "^20.17.6",
    "@types/wrap-ansi": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concat": "^1.0.3",
    "eslint": "^8.56.0",
    "fs-extra": "^11.2.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nx": "17.2.0",
    "rimraf": "^5.0.10",
    "typescript": "~5.2.2"
  }
}